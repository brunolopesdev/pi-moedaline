<!-- CONTATO -->
<section class="contato section" id="contato">
    <h2 class="section-title">Contato</h2>
    <form id="fs-frm" name="simple-contact-form" accept-charset="utf-8" action="#" method="post">
        <fieldset id="fs-frm-inputs">
          <label for="full-name">Nome:</label>
          <input type="text" name="name" id="full-name" placeholder="Nome completo" required="">
          <label for="email-address">Email:</label>
          <input type="email" name="_replyto" id="email-address" placeholder="email@email.com.br" required="">
          <label for="message">Mensagem</label>
          <textarea rows="5" name="message" id="message" placeholder="Digite sua mensagem" required=""></textarea>
          <input type="hidden" name="_subject" id="email-subject" value="Contact Form Submission">
        </fieldset>
        <input type="submit" value="Submit">
      </form>
</section>